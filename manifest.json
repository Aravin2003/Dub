{
    "name": "Dub",
    "description": "Download all your favourite youtube videos easily!",
    "version":"1.0",
    "permissions":["storage" , "tabs" , "activeTab", "declarativeContent","downloads","notifications"],
    "host_permissions":["https://*.youtube.com/*"],
    "background":
    {
        "service_worker":"background.js"
    },
    "web_accessible_resources": [
      {
        "resources": [
          "assets/download.png"
        ],
        "matches": ["https://*.youtube.com/*"]
      }
    ],
    "action":
    {
        "default_popup":"popup.html",
        "default_icon":
        {
          "16": "assets/favicon16.png",
      "32": "assets/favicon32.png"
        }
    },
    "icons": {
      "16": "assets/favicon16.png",
      "32": "assets/favicon32.png",
      "48": "assets/icon48.png",
      "128": "assets/icon128.png"
      },
    "manifest_version":3
}